<HTML>
<HEAD>
<TITLE></TITLE>

<SCRIPT LANGUAGE="JavaScript">

// preload images:
if (document.images) {
red_arrow_off = new Image(12,14); red_arrow_off.src = "red_arrow_off.gif";
red_arrow_on =  new Image(12,14); red_arrow_on.src =  "red_arrow_on.gif";
}

function hiLite(imgName,imgObjName) {
if (document.images) {
  document.images[imgName].src = eval(imgObjName + ".src");
}}

function nada() {
}

//--></SCRIPT>

</HEAD>
<BODY>

   <IMG SRC="red_arrow_off.gif" WIDTH="12" HEIGHT="14" BORDER="0" 
   ALT="" NAME="img04">
<A HREF="javascript:nada()" 
   onMouseOver="hiLite('img04','red_arrow_on')"
   onMouseOut="hiLite('img04','red_arrow_off')">Home</A>
<br>

   <IMG SRC="red_arrow_off.gif" WIDTH="12" HEIGHT="14" BORDER="0" 
   ALT="" NAME="img05">
<A HREF="javascript:nada()" 
   onMouseOver="hiLite('img05','red_arrow_on')"
   onMouseOut="hiLite('img05','red_arrow_off')">Link</A>
<br>

   <IMG SRC="red_arrow_off.gif" WIDTH="12" HEIGHT="14" BORDER="0" 
   ALT="" NAME="img06">
<A HREF="javascript:nada()" 
   onMouseOver="hiLite('img06','red_arrow_on')"
   onMouseOut="hiLite('img06','red_arrow_off')">Email</A>
<br>

</BODY>
